{
  "appId": "org.rsimmons.voracious",
  "directories": {
    "buildResources": "buildResources"
  },
  "files": [
    "**/*",
    {
      "from": "../build",
      "to": ".",
      "filter": ["**/*"]
    }
  ],
  "asar": false,
  "asarUnpack": [
    "resources/bin/**"
  ],
  "npmRebuild": false,
  "extends": null,
  "mac": {
    "target": "zip"
  },
  "nsis": {
    "artifactName": "${productName}-${version}-windows.${ext}"
  }
}
